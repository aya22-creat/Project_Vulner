{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=your_production_server;Database=ProjectVulnDb;User Id=your_user;Password=your_password;TrustServerCertificate=True;MultipleActiveResultSets=True"
  },
  "AiService": {
    "BaseUrl": "http://your-ai-service:8000",
    "TimeoutSeconds": "60"
  },
  "Limits": {
    "MaxFileSizeBytes": "1048576",
    "MaxRepoSizeBytes": "104857600"
  },
  "RateLimiting": {
    "RequestsPerMinute": "10",
    "RequestsPerHour": "100"
  }
}
